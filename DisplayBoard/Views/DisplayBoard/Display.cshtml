@model DisplayBoardConfigurationModel
@{
    Layout = null;
}
<html>
  <head>
    <title>Display Board</title>
    <link href="/styles.css" rel="stylesheet"></link>
  </head>
  <body>
    <div id="slide-template">
      <div class="slide">
        <div class="background">
        loading...
        </div>
        <iframe class="content">I'm an iframe!</iframe>
        <div class="overlay">
          <div class="title"></div>
        </div>
      </div>
    </div>
    <div id="slide-deck"></div>
    <div id="stage"></div>
    <script id="configuration" type="text/json">@Html.Raw(Model.ToJson())</script>
    <script src="http://code.jquery.com/jquery-1.10.2.min.js" type="text/javascript"></script>
    <script src="/scripts.js" type="text/javascript"></script>
  </body>
</html>